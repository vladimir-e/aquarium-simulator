{
  "name": "aquarium-simulator",
  "version": "1.0.0",
  "description": "Comprehensive aquarium ecosystem simulation engine",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint 'src/**/*.{ts,tsx}' 'game/**/*.{ts,tsx}'",
    "lint:fresh": "eslint --no-cache 'src/**/*.{ts,tsx}' 'game/**/*.{ts,tsx}'",
    "ui": "vite",
    "ui:build": "vite build",
    "dev:game": "vite --config vite.game.config.ts",
    "build:game": "vite build --config vite.game.config.ts",
    "preview:game": "vite preview --config vite.game.config.ts",
    "test:game": "vitest run --config vitest.game.config.ts",
    "test:game:watch": "vitest --config vitest.game.config.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vladimir-e/aquarium-simulator.git"
  },
  "keywords": [
    "aquarium",
    "simulation",
    "ecosystem"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vladimir-e/aquarium-simulator/issues"
  },
  "homepage": "https://github.com/vladimir-e/aquarium-simulator#readme",
  "dependencies": {
    "framer-motion": "^12.31.0",
    "immer": "^11.1.3",
    "lucide-react": "^0.562.0",
    "pixi.js": "^8.16.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.0.13",
    "@testing-library/react": "^16.1.0",
    "@types/node": "^25.0.6",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@typescript-eslint/eslint-plugin": "^8.52.0",
    "@typescript-eslint/parser": "^8.52.0",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^4.0.17",
    "eslint": "^9.39.2",
    "happy-dom": "^16.2.3",
    "tailwindcss": "^4.0.13",
    "typescript": "^5.9.3",
    "vite": "^6.0.7",
    "vitest": "^4.0.17"
  }
}
